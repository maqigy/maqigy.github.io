<!DOCTYPE html>
<html>
    <head>
        <link href="/styles/main.css" rel="stylesheet" type="text/css" />
    </head>

    <body>
        <%- include('partials/nav'); %>
        <h1>Welcome to your profile, <%= id %></h1>
        <p>You are <%= data.age%>, and you are <%=data.job%></p>
        <h2>Hobbies</h2>
        <ul>
            <% data.hobbies.forEach(function(item){ %>
                <li><%= item %></li>
            <% }); %>
        </ul>
    </body>
</html>